{
    "version": "0.2.0",
    "configurations": [  
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "LD_LIBRARY_PATH": "${workspaceFolder}/tvm/BUILD/DEBUG/libtvm/BUILD/DEBUG/lib",
                "PYTHONPATH": "${workspaceFolder}/tvm/python",
                "TVM_LIBRARY_PATH": "${workspaceFolder}/tvm/BUILD/DEBUG/BUILD"
            }
        }
    ]
}